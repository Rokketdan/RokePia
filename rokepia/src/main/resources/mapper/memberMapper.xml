<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<mapper namespace="com.example.rokepia.repository.MemberMapper">
	<insert id="saveMember">
		INSERT INTO member (
			member_email,
			name,
			password,
			gender
		) VALUES (
			#{member_email},
			#{name},
			#{password},
			#{gender}
		)
	</insert>
	
	<select id="findMember" resultType="com.example.rokepia.model.Member">
		select * from member where member_email = #{member_email}
	</select>
	
	
</mapper>